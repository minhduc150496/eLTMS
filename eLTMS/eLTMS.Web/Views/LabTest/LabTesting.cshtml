
@{
    ViewBag.Title = "LabTesting";
    Layout = "~/Views/Shared/_LayoutPageWarehouseManagement.cshtml";
}

<style>
    .modal {
        overflow: auto !important;
    }
</style>
<section class="content-header">
    <h1>
        Quản Lý Kết Qủa Xét Nghiệm
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Tables</a></li>
        <li class="active">Data tables</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">

    <div class="box">
        @*<div class="box-header">
                <h3 class="box-title">Danh Sách Các Xét Nghiệm</h3>
            </div>*@
        <!-- /.box-header -->
        <div class="box-body">
            <button class="btn btn-success" id="btnViewLabTesting"> Lab Testing</button>
            <button class="btn btn-success" id="btnSaveLabTestingIndex"> Xử Lý</button>
            <input type="file" id="input" class="btn btn-info" accept="application/vnd.ms-excel, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />

        </div>
        <table style="width:100%" class="table table-bordered">
            <thead>
                <tr>
                    @*<th>#</th>*@
                    <th>Labtesting ID</th>
                    <th>Index Name</th>
                    <th>Index Value</th>
                    <th>Trạng Thái</th>
                    <th>Mức Trung Bình</th>
                    <th>Đơn Vị</th>

                </tr>
            </thead>
            <tbody id="tblData1">

                <tr id="template-row" style="display:none;">
                    @*<td class="colIndex"></td>*@
                    <td class="colId"></td>
        <td class="colName"></td>
        <td class="colValue"></td>
        <td class="colStatus"></td>
        <td class="colNomal"></td>
        <td class="colUnit"></td>
                   
                    @*<td class="colCode"></td>
                    <td class="colName"></td>
                    <td class="colQuantity"><input type="number" min="1" class="txtQuantity form-control" value="" /></td>
                    <td class="colUnit"></td>
                    <td class="colNote"><input type="text" class="txtNote form-control" value="" /></td>*@

                </tr>
            </tbody>


        </table>
    </div>
</section>
<!-- /.content -->
<!-- Modal -->


<div id="myModalLabTestingData" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" id="lblPopupTitle">Thêm Mới Loại Xét Nghiệm</h4>
            </div>
            <button class="btn btn-primary" id="btnSaveLabTesting">Lưu</button>
            <div class="modal-body">
                <table style="width:100%" class="table table-bordered">
                    <thead>
                        <tr>
                            @*<th>#</th>*@
                            <th>Mã lịch hẹn</th>
                            <th>Tên xét nghiệm</th>
                            <th>Vị trí</th>
                        </tr>
                    </thead>
                    <tbody id="tblData">

                        <tr id="template1-row" style="display:none;">
                            @*<td class="colIndex"></td>*@
                            <td class="colCode"></td>
                            <td class="colName"></td>
                            <td class="colSlot"><input type="number" min="1" max="10" class="txtSlot form-control" value="" readonly /></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>

    </div>
</div>

<div id="myModalLabTesting" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" id="lblPopupTitle">Danh sách các yêu cầu xét nghiệm</h4>
            </div>
            <br />
            <button class="btn btn-success" id="btnAddNewLabTesting">Xử lý</button>
            <div class="modal-body">
                <form class="form-horizontal">
                    <table id="example2" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>Mã lịch hẹn</th>
                                <th>Tên xét nghiệm</th>
                                <th>Mẫu xét nghiệm</th>
                                <th>Trạng thái</th>
                                <th>Vị trí</th>
                                <th>Thao tác</th>
                            </tr>
                        </thead>
                     <tbody id="tblDataLabTesting"></tbody>
                    </table>
                    <div style="margin-left:35%" id="pagination" class="pagination"></div>
                </form>
            </div>
            <div class="modal-footer">

                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>




<script id="dataLabTesting-template" type="x-tmpl-mustache">
    <tr>
        <td>{{Getting}}</td>
        <td>{{Name}}</td>
        <td>{{Group}}</td>
        <td>{{Status}}</td>
        <td>{{Slot}}</td>
        <td>
            <button class="btn btn-danger btn-deleteSampleGroup" data-id="{{LabTestingId}}"><i class="fa fa-trash"></i></button>

        </td>
    </tr>
</script>

